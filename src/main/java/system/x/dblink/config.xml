<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD//Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
<properties resource="portal/org/dblink/credenciales.properties"/>  

	<typeAliases>
		<typeAlias alias="pojocia" type="system.x.pojo.Cias"/>
  	</typeAliases> 

  

<environments default="development">
<environment id="development">
<transactionManager type="JDBC"/>	

<dataSource type="POOLED">
<property name="driver"	 		value="${driver}"/>
<property name="url" 			value="${puente}${server}:${port}/${db}"/>
<property name="username" 		value="${usuario}"/>
<property name="password" 		value="${pws}"/>	


</dataSource>
</environment>
</environments>

<mappers>
	<mapper resource="portal/org/pojo/Cias.xml"/>
</mappers>

 
</configuration>